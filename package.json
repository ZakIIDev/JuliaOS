    "figlet": "^1.9.4",
    "fs-extra": "^11.3.2",
    "hardhat": "^2.28.0",
    "inquirer": "^8.2.7",
    "langchain-core": "^0.0.4",
    "log-symbols": "^4.1.0",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "matrix-js-sdk": "^37.13.0",
    "merkletreejs": "^0.6.0",
    "node-fetch": "^3.3.2",
